<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>fil_rouge</display-name>
 	<servlet>
	    <servlet-name>order</servlet-name>
	    <servlet-class>com.galamome.servlets.order</servlet-class>
	    <init-param>
	        <param-name>auteur</param-name> 
	        <param-value>Guillaume</param-value> 
	    </init-param>
	</servlet>
	<servlet-mapping>
	    <servlet-name>order</servlet-name>
	    <url-pattern>/creationCommande</url-pattern>
	</servlet-mapping>
 	<servlet>
	    <servlet-name>client</servlet-name>
	    <servlet-class>com.galamome.servlets.client</servlet-class>
	    <init-param>
	        <param-name>auteur</param-name> 
	        <param-value>Guillaume</param-value> 
	    </init-param>
	</servlet>
	<servlet-mapping>
	    <servlet-name>client</servlet-name>
	    <url-pattern>/creationClient</url-pattern>
	</servlet-mapping> 	
</web-app>